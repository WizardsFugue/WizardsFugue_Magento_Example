{
	"name":     "WizardsFugue/magento_example",
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.firegento.com"
        },
        {
            "type": "composer",
            "url": "http://packages.base.cotya.com/"
        }
    ],
	"autoload":{
        "files": [
            "./vendor/connect20/Mage_Core_Modules/app/code/core/Mage/Core/functions.php"
        ],
        "psr-0": {
            "": "src/"
        },
	    "classmap": [
                "vendor/connect20"
	    ]
	},
    "include-path": [
        "./mage/",
        "./vendor/connect20/Mage_Core_Modules/app/code/core",
        "./vendor/connect20/lib_zf/lib/",
        "./vendor/connect20/lib_zf_locale/lib/"
    ],
	"require":  {
	    "wizards-fugue/base": "dev-master",
        "connect20/mage_all_latest": "1.7.*",
        "symfony/console": "2.4.*",
        "symfony/finder": "2.4.*",
        "symfony/dom-crawler": "2.4.*",
        "symfony/css-selector": "2.4.*",
        "magento-hackathon/magento-composer-installer":    "dev-master"
	},
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra":{
        "magento-deploystrategy": "symlink",
        "magento-force": true,
        "magento-root-dir": "not_used/"
    }

    

}
